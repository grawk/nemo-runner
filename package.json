{
  "name": "nemo-runner",
  "version": "2.0.0-alpha.1",
  "description": "Wrapper to run nemo/mocha suites",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && npm run nemo",
    "nemo": "DEBUG=nemo-runner* ./bin/nemo-runner -B test -G @suite1",
    "nemo:no:selenium:promises": "SELENIUM_PROMISE_MANAGER=0 DEBUG=nemo-runner* ./bin/nemo-runner -B test -P pay -D",
    "nemo:debug": "DEBUG=nemo-runner ./bin/nemo-runner -B test -G @suite1 --debug-brk --inspect",
    "nemo:parallel": "DEBUG=nemo-runner* ./bin/nemo-runner -B test -G @suite1,@suite2,@suite3,@suite4,@suitex -P chrome,firefox",
    "nemo:parallel:data": "DEBUG=nemo-runner* ./bin/nemo-runner -B test -P search,pay -D",
    "nemo:search": "SELENIUM_PROMISE_MANAGER=0 DEBUG=nemo-runner* ./bin/nemo-runner -B test -P search -D",
    "nemo:pay": "SELENIUM_PROMISE_MANAGER=0 DEBUG=nemo-runner* ./bin/nemo-runner -B test -P pay -D",
    "nemo:form": "SELENIUM_PROMISE_MANAGER=0 DEBUG=nemo-runner* ./bin/nemo-runner -B test -P form",
    "nemo:xunit": "DEBUG=nemo-runner* ./bin/nemo-runner -B test -G @suite1 -P xunit",
    "lint": "eslint ./bin/* ./lib/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paypal/nemo-runner.git"
  },
  "bin": {
    "nemo-runner": "./bin/nemo-runner",
    "_nemo-runner": "./bin/_nemo-runner"
  },
  "keywords": [
    "nemo",
    "mocha",
    "selenium",
    "parallel"
  ],
  "author": "grawk <mattedelman@gmail.com>",
  "contributors": [
    "grawk <mattedelman@gmail.com>",
    "Kurt Weiberth <kurt.weiberth@gmail.com>",
    "Ethan Godt <ethan@ethangodt.com>"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/grawk/nemo-runner/issues"
  },
  "homepage": "https://github.com/grawk/nemo-runner#readme",
  "dependencies": {
    "async": "^2.5.0",
    "commander": "^2.9.0",
    "debug": "^2.6.3",
    "filenamify": "^2.0.0",
    "glob": "^7.1.1",
    "lodash.merge": "^4.6.0",
    "mocha": "^3.2.0",
    "mochawesome": "^2.3.1",
    "moment": "^2.19.2",
    "threads": "^0.7.2",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-plugin-es6-recommended": "^0.1.2",
    "nemo": "^3",
    "nemo-view": "^3"
  }
}
